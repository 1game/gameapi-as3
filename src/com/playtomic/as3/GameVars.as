package com.playtomic.as3 {		public final class GameVars {				private static const GAMEVARS:String = "gamevars";		private static const LOAD:String = "load";		private static const SINGLE:String = "single";				/**		 * Loads your GameVars 		 * @param	callback	Your function to receive the data:  callback(gamevars:Object, response:Response)		 */		public static function load(callback:Function):void {			ServiceRequest.load(GAMEVARS, LOAD, loadComplete, callback, null);		}				/**		 * Loads a single GameVar		 * @param	name	The name of the var you want to laod		 * @param	callback	Your function recieve the data:  callback(gamevar:Object, response:Response)		 */		public static function loadSingle(name:String, callback:Function):void {			var postdata:Object = {				name: name			}						ServiceRequest.load(GAMEVARS, SINGLE, loadComplete, callback, postdata);		}				/**		 * Processes the response received from the server, returns the data and response to the user's callback		 * @param	callback	The user's callback function		 * @param	data		The data returned from the server		 * @param	status		The request status returned from the esrver (1 for success)		 * @param	errorcode	The errorcode returned from the server (0 for none)		 */		private static function loadComplete(callback:Function, data:Object = null, response:Response = null):void {			if(callback == null)				return;							callback(data, response);		}	}}