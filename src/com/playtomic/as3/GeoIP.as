package com.playtomic.as3 {		public final class GeoIP {				private static const SECTION:String = "geoip";		private static const LOAD:String = "lookup";				/**		 * Performs a country lookup on the player IP address		 * @param	callback	Your function to receive the data:  callback(data:Object, response:Response);		 * @param	view	If it's a view or not		 */		public static function lookup(callback:Function):void {			ServiceRequest.load(SECTION, LOAD, lookupComplete, callback);		}					/**		 * Processes the response received from the server, returns the data and response to the user's callback		 * @param	callback	The user's callback function		 * @param	data		The data returned from the server		 * @param	status		The request status returned from the esrver (1 for success)		 * @param	errorcode	The errorcode returned from the server (0 for none)		 */		private static function lookupComplete(callback:Function, data:Object = null, response:Response = null):void {						if(callback == null)				return;						callback(data, response);		}	}}